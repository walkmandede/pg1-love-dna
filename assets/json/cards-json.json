      "interpretations": [
        { "id": "1", "label": "They're probably busy." },
        { "id": "2", "label": "Something's shifted." },
        { "id": "3", "label": "I want to feel wanted." },
        { "id": "4", "label": "Good things don't need forcing." }
      ]
    },
